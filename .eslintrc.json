{
  "env": {
    "commonjs": true,
    "es2021": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": ["error", { "args": "after-used" }],
    "class-methods-use-this": "off"
  },
  "plugins": ["jest"]
}
