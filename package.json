    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.14",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "redis": "^4.0.6",
    "redis-mock": "^0.56.3",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
