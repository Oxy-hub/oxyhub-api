  "description": "Api for oxyhub",
  "repository": "https://github.com/Oxy-hub/oxyhub-api",
  "main": "./src/server.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node ./src/server.js",
    "start": "cross-env NODE_ENV=development nodemon",
